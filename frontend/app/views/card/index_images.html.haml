- if @warnings.present?
  .row
    .col-12.warnings
      - @warnings.each do |warning|
        .warning= warning
- if !@empty_page
  = render 'summary'
  .row.multicard_row
    - @cards.each_with_index do |(card, printings), i|
      .col-12.col-sm-6.col-md-4.card_picture_cell{class: "card_#{i % 3}_of_3 card_#{i % 2}_of_2"}
        = link_to_card(card) do
          = render 'card/picture', card: card
  = render "pagination", cards: @cards
